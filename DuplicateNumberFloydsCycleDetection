class Solution {
    public int findDuplicate(int[] nums) {
        //Floyd's Tortoise & Hare (Cycle Detection)
        //Find the duplicates Using Floyd's detection method
        int tortoise = nums[0];
        int hare= nums[0];
        //Phase1: Find intersection point of tortoise and hare.
        do
        {
            tortoise=nums[tortoise];
            hare=nums[nums[hare]];
        }while(tortoise!=hare);
        //Phase 2 for cycle detection:
        tortoise=nums[0];
        while(tortoise!=hare)
        {
             tortoise=nums[tortoise];
             hare=nums[hare];
        }
return hare;

}
}
Time Complexity: O(n)
Space Complexity: O(1)
